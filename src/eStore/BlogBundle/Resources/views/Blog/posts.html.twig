{% extends('eStoreBlogBundle:Default:base.html.twig')%}


{% block content %}

	{% for post in posts %}
	<div class = 'row'>
		<div class = 'box'>
			<div class = 'col-lg-12 text-center'>
				<h3>{{ post.title }}</h3>
			<hr>
				{{ post.body }}
			<hr>
				Authored by {{ post.user }}
			</div>
		</div>
	</div>
	{% endfor %}
	

{% endblock %}